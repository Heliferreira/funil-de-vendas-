{
  "name": "funnel-server",
  "version": "1.0.0",
  "type": "module",
"scripts": {
  "dev": "nodemon src/index.js",
  "start": "node dist/index.js",
 "build": "node -e \"if(require('fs').existsSync('dist')){require('fs').rmSync('dist',{recursive:true,force:true})}\" && node ./node_modules/esbuild/bin/esbuild src/index.js --bundle --platform=node --format=esm --packages=external --outfile=dist/index.js",
  "postinstall": "node ./node_modules/prisma/build/index.js generate"
},
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "esbuild": "^0.23.0",
    "nodemon": "^3.1.0",
    "prisma": "^5.20.0",
    "rimraf": "^5.0.7"
  }
}
